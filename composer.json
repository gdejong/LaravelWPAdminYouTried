{
    "name": "gdejong/LaravelWPAdminYouTried",
    "type": "library",
    "description": "Bait for any configured login page.",
    "keywords": [
        "gdejong",
        "LaravelWPAdminYouTried"
    ],
    "homepage": "https://github.com/gdejong/LaravelWPAdminYouTried",
    "license": "MIT",
    "authors": [
        {
            "name": "Gijs de Jong"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : "~5.6|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=5.4.3",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "gdejong\\LaravelWPAdminYouTried\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "gdejong\\LaravelWPAdminYouTried\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "gdejong\\LaravelWPAdminYouTried\\LaravelWPAdminYouTriedServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
